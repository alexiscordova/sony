include ../../../common/html/jade-helpers.jade
doctype 5
include ../../../common/html/ieconditionals.html

head
  title Feature Slideshow B1 Demo
  include ../../../common/html/head.jade
  style
    .fs-spacing {
      height: 250px;
    }

    .fs-spacing:nth-of-type(1) {
      background-color: #1a1a1a;
    }

    .fs-spacing:nth-of-type(2) {
      background-color: #333333;
    }

    .fs-spacing:nth-of-type(3) {
      background-color: #4d4d4d;
    }

    .fs-spacing:nth-of-type(4) {
      background-color: #666666;
    }

    .fs-spacing:nth-of-type(5) {
      background-color: gray;
    }

    .fs-spacing:nth-of-type(6) {
      background-color: #999999;
    }

    .fs-spacing:nth-of-type(7) {
      background-color: #b3b3b3;
    }

    .fs-spacing:nth-of-type(8) {
      background-color: #cccccc;
    }

    .fs-spacing:nth-of-type(9) {
      background-color: #e6e6e6;
    }

    .fs-spacing:nth-of-type(10) {
      background-color: white;
    }

body
  #page-wrap-outer.page-wrap-outer
    #page-wrap-inner.page-wrap-inner
      e = {"locals":locals, "data":data('packages/pages/data/global-header-footer/default-header.json'), "sample_session_data":data('packages/pages/data/global-header-footer/sample_session_data.json'), "sample_search_results":data('packages/pages/data/global-header-footer/sample_search_results.json')}
      !{partial('packages/modules/global-header-footer/html/global-header.jade', e)}
      #main(role='main')

        article.fs-spacing
        article.fs-spacing
        article.fs-spacing
        article.fs-spacing
        article.fs-spacing
        article.fs-spacing
        article.fs-spacing

        +partial('feature-slideshow-b1-x00/html/feature-slideshow-b1-x00.jade', 'packages/modules/feature-slideshow-b1-x00/demo/data/feature-slideshow-default.json')

        article.fs-spacing
        article.fs-spacing
        article.fs-spacing

  include ../../../common/html/foot.jade
</html>
