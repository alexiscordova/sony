cat1 = data.categoryLinks[0]
cat2 = data.categoryLinks[1]
cat3 = data.categoryLinks[2]

//section.tcc-wrapper.container.full-bleed-no-max.m-full-bleed(id=data.id, data-tcc-mode="#{data.moduleMode}")

section.ulinks-cat-wrapper.container.full-bleed-no-max.m-full-bleed
  .ulinks-header-wrapper.container
    .ulinks-header.grid

  section.ulinks-body-wrapper.container
    div.ulinks-body.grid
      div.ulinks-content-module.ulink-list.span3.m-span3.table-center-wrap
        h5.t7-light #{cat1.title}
        ul.utility-link
          - for(var i=0; i < cat1.links.length; i++)
            linkLabel = cat1.links[i].label
            linkUrl = cat1.links[i].url
            li
              a.lt3(href="#{linkUrl}", title="#{linkLabel}") #{linkLabel}
      div.ulinks-content-module.ulink-list.span3.m-span3.table-center-wrap
        h5.t7-light #{cat2.title}
        ul.utility-link
          - for(var i=0; i < cat1.links.length; i++)
            linkLabel = cat2.links[i].label
            linkUrl = cat2.links[i].url
            li
              a.lt3(href="#{linkUrl}", title="#{linkLabel}") #{linkLabel}
      div.ulinks-content-module.ul-content-box-outer.span6.table-center-wrap
        .ul-content-box.clearfix
          .first
            img(src=cat3.imgSrc).ut-block-img
          .last
            h5.t5-light #{cat3.title}
            p.p3 #{cat3.body}
            a.lt3(href="#{cat3.url}", title="#{cat3.label}") #{cat3.label}