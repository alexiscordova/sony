section.editorial-hs-wrapper.container.full-bleed-no-max.m-full-bleed
  h1 Editorial Hotspot E6 UI
  h5 Button Example
  .hotspot-container-example.hspot-container
    .hspot-outer(data-x="0",data-y="0")
      .hspot-core
        .hspot-indicator
          img(src="img/editorial-hotspots-e6/hotspot-indicator.png");
      .overlay-variant1.hidden
        .top top
        .middle middle
        .footer foot
    .hspot-outer(data-x="200",data-y="200")
