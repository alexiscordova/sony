include ../common/html/jade-helpers.jade
doctype 5
include ../common/html/ieconditionals.html

head
  title Vaio Duo
  include ../common/html/head.jade

body
  #page-wrap-outer.page-wrap-outer
    #page-wrap-inner.page-wrap-inner
      e = {"locals":locals, "data":data('packages/pages/data/global-header-footer/default-header.json'), "sample_session_data":data('packages/pages/data/global-header-footer/sample_session_data.json'), "sample_search_results":data('packages/pages/data/global-header-footer/sample_search_results.json')}
      !{partial('packages/modules/global-header-footer/html/global-header.jade', e)}
      #main(role='main')

        //- Cookie Alert
        +partial("cookie-policy/html/cookie-policy.jade", "packages/pages/data/cookie-policy/cookie-policy-example.json")
          
        //- D3. Product Intro Plate
        +partial('primary-tout-p1-d7/html/primary-tout-p1-d7.jade', 'packages/pages/data/primary-tout-p1-d7/pdp-LD-R-duo.json')

        //- D1.D2 - Product Summary
        +partial('product-summary-d1-d2/html/product-summary-d1-d2.jade', 'packages/pages/data/product-summary-d1-d2/vaio-duo-pdp.json')

        //- E6.C2 Editorial + Hotspots
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-hotspots-e6/e6.c2.container-duo.json')
          
        //- E4.C2 MediaRight
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-slideshows-e4/vaio-duo-pdp.json')       

        //- E1 with E5 as submodule and E1's as E5's submodules - Editorial Chapters
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-layouts-e1/full-left-102-D-L-duo1.json')

        //- E1
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-layouts-e1/full-inner-left-57-D-L-vaio-duo.json')

        //- E01.01
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-layouts-e1/tout-3up-444-D-C-vaio-duo.json')

        //- Image Sequence
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/modules/editorial-transition-b1/demo/data/layouts-editorial-transition.json')

        //- E1 with E5 as submodule and E1's as E5's submodules - Editorial Chapters
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-layouts-e1/full-left-102-D-L-duo2.json')

        //- E1_? - Editorial Layouts
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-layouts-e1/tout-3up-444-D-C-vaio-fit.json')

      +scrollToTop

      e = {"locals":locals, "data":data('packages/pages/data/global-header-footer/default-footer.json'), "sample_session_data":data('packages/pages/data/global-header-footer/sample_session_data.json')}
      !{partial('packages/modules/global-header-footer/html/global-footer.jade', e)}

  include ../common/html/foot.jade
</html>

