include ../common/html/jade-helpers.jade
doctype 5
include ../common/html/ieconditionals.html

head
  title VAIO Pro
  include ../common/html/head.jade

body
  #page-wrap-outer.page-wrap-outer
    #page-wrap-inner.page-wrap-inner
      e = {"locals":locals, "data":data('packages/pages/data/global-header-footer/default-header.json'), "sample_session_data":data('packages/pages/data/global-header-footer/sample_session_data.json'), "sample_search_results":data('packages/pages/data/global-header-footer/sample_search_results.json')}
      !{partial('packages/modules/global-header-footer/html/global-header.jade', e)}
      #main(role='main')

        //- P2_B1 - Product Intro Plate
        +partial('primary-tout-p1-d7/html/primary-tout-p1-d7.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/p1.d7.json')

        //- D1.D2 - Product Summary
        +partial('product-summary-d1-d2/html/product-summary-d1-d2.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/d1.d2.json')

        //- Feature Slideshow
        +partial('feature-slideshow-b1/html/feature-slideshow-b1.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/b1.json')

        //- E6.C2 Editorial + Hotspots
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/e1_1.json')

        //- E1.V1 - Editorial Layouts
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/e1_2.json')

        //- E5 with E1s
        +partial('editorial-chapters-e5/html/editorial-chapters-e5.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/e5.json')

        //- E1_? - Editorial Layouts
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/e1_3.json')

        //- E4_A1 - Editorial Slideshow
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/e1_4.json')

        //- E1 full inner right
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/e1_5.json')

        //- E1.N2
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/e1_6.json')

        //- S3_B1 - Secondary Tout
        +partial("secondary-touts-s1-s4/html/secondary-tout-s3.jade", "packages/pages/data/Batch1-Nurun-04-Pro/s3.json")

        //- D5_? - Product Details
        +partial('product-details-d5/html/product-details-d5.jade', 'packages/pages/data/Batch1-Nurun-04-Pro/d5.json')


      +scrollToTop

      e = {"locals":locals, "data":data('packages/pages/data/global-header-footer/default-footer.json'), "sample_session_data":data('packages/pages/data/global-header-footer/sample_session_data.json')}
      !{partial('packages/modules/global-header-footer/html/global-footer.jade', e)}

  include ../common/html/foot.jade
</html>

