      !{partial('packages/modules/global-header-footer/html/global-header.jade', e)}
      #main(role='main')

        //- D1-2
        +partial('product-summary-d1-d2/html/product-summary-d1-d2.jade', 'packages/pages/data/product-summary-d1-d2/ex645-pdp.json')

        //- D4
        +partial('pdp-slideshow-d4/html/pdp-slideshow-d4.jade', 'packages/modules/pdp-slideshow-d4/demo/data/ex645-pdp.json')

        //- E1 ( mediaright 66 )
