        //- E1( tout 2up vertical )
        +partial('editorial-layouts-e1/html/editorial-layouts-e1.jade', 'packages/pages/data/editorial-layouts-e1/tout-2up-vertical-1551-D-C-ex645.json')

        //- D5
        +partial('product-details-d5/html/product-details-d5.jade', 'packages/pages/data/product-details-d5/product-details-d5-EX645.json')

        //- D6
        +partial('reviews-awards-d6/html/reviews-awards-d6.jade', 'packages/pages/data/reviews-awards-d6/pdp-ex645.json')

        //- G1 ( 4up )
        +partial('related-products-g1/html/related-products-g1.jade', 'packages/modules/related-products-g1/demo/data/ex645-pdp.json')

        //- T1
        +partial('tertiary-tout-t1-t5/html/tertiary-tout-t1-t5.jade', 'packages/pages/data/tertiary-tout-t1-t5/tertiary-tout-EX645.json')

        //- D8
        //- +partial('utility-links-pdp-d8/html/utility-links-pdp-d8.jade', "packages/pages/data/utility-links-pdp-d8/homepage.json")

        //- Footnotes
        +partial("footnotes/html/footnotes.jade", "packages/modules/footnotes/demo/data/homepage-en.json")

        //- Recently Viewed
        +partial('recently-viewed/html/recently-viewed.jade', 'packages/pages/data/recently-viewed/ex645-pdp.json')

      e = {"locals":locals, "data":data('packages/pages/data/global-header-footer/default-footer.json'), "sample_session_data":data('packages/pages/data/global-header-footer/sample_session_data.json')}
