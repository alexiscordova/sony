mixin descriptors()
  for name in locals.pages()
    li: a( href = '../build/debug/' + name) !{name.replace(/.html/g, '').replace(/-/g, ' ')}

include ../../common/html/ieconditionals.html
doctype 5

include docs-head.html
body( data-spy = "scroll", data-target = ".bs-docs-sidebar" )
  include docs-nav.html

  header.jumbotron.subhead#overview
    div.container
      h1 Archetypal Pages
      p Modules Working Together.

  
  div.container
    ul
      +descriptors()
    
  include docs-foot.html
</html>