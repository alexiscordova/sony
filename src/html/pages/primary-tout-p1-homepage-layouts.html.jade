title: P1. Primary Tout Module - Homepage
description: Examples of the Primary Tout homepage layouts
---
include ../partials/includes/jade-helpers.jade
doctype 5
!{partial('includes/ieconditionals.html')}

l = {"locals":locals}
  head
    !{partial('includes/head.html.jade', l)}
  
  body
    h6 homepage dark text left 
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-D-L-nex.json')
    <br>
    
    h6 homepage dark text left eyebrow and body text
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-D-L-walkman.json')
    <br>
    
    h6 homepage light text left eyebrow and body text
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-L-L-ion.json')
    <br>
    
    h6 homepage light text centered with body text
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-L-C-ion.json')
    <br>
    
    h6 homepage light text centered with secondary box light text dark background
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-L-C-secondary-L-ion.json')
    <br>
    
    h6 homepage dark text left with secondary box dark text light background
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-D-L-secondary-D-ion.json')
    <br>
    
    h6 homepage light quote left with secondary box light text dark background
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-L-L-secondary-D-rx100-quote.json')
    <br>
    
    h6 homepage light quote centered with secondary box dark text light background
    +partial('primary-tout-p1-d7.html.jade', 'primary-tout-p1-d7/homepage-L-C-secondary-D-rx100-quote.json')
    <br>
  
    !{partial('includes/foot.html.jade', l)}
