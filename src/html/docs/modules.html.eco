<!--[if (gt IE 9)|!(IE)]><!--> <html class="modern" lang="en"> <!--<![endif]-->

	<%-@partial('includes/docs_head.html')%>
	<body data-spy="scroll" data-target=".bs-docs-sidebar">
		<%-@partial('includes/docs_nav.html')%>

		<div class="jumbotron subhead">
		  <div class="container-px-width">
		    <h2 class="bs-docs-heading">Modules</h2>
		  </div>
		</div>
		
		<div class="container-px-width">
			<ul>
				<li><!-- not sure how to add the header+footer modules to a single page, so I'm just hard-coding it --> 
					<a href="../build/debug/global-header-footer.html">Global Header / Footer</a>: Example page with global Header &amp; Footer.
				</li>
			<% for name in @modulepages(): %>
				<% if name != 'index.html.eco' && name.indexOf('_module.html')!= -1 : %>
					<li><a href='../build/debug/<%= name.replace(/(.eco|.hb)/g, '') %>'><%= @title(name) || name.replace(/(.html.eco|.html.hb)/g, '').replace(/(_|-)/g, ' ') %></a>: <%= @desc(name) %></li><% end %><% end %>
			</ul>
		</div>
		
		<%-@partial('includes/docs_foot.html')%>
	</body>
</html>
