// Tooltips and popovers
// -------------------------
.TOOLTIPS-AND-POPOVERS{} // this is just here for a SublimeText code-folding collapse point
	$tooltipColor:            rgb(255,255,255) !default;
	$tooltipBackground:       rgb(0,0,0) !default;
	$tooltipArrowWidth:       5px !default;
	$tooltipArrowColor:       $tooltipBackground !default;

	$popoverBackground:       rgb(255,255,255) !default;
	$popoverArrowWidth:       10px !default;
	$popoverArrowColor:       rgb(255,255,255) !default;
	$popoverTitleBackground:  darken($popoverBackground, 3%) !default;

