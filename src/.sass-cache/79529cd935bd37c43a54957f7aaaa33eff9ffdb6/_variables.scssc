	$popoverArrowOuterWidth:  $popoverArrowWidth + 1 !default;
	$popoverArrowOuterColor:  rgba(0,0,0,.25) !default;





// GRID
// -----------------------------
// 

.GRID{} // this is just here for a SublimeText code-folding collapse point

	$gridColumns:             12 !default; 
	$grid5Columns: 			  5;

	// Default 940px grid @980px-1199px
	$gridColumnWidth:         54px !default;
	$gridGutterWidth:         30px !default;
	$gridRowWidth:            ($gridColumns * $gridColumnWidth) + ($gridGutterWidth * ($gridColumns - 1)) !default;

	// 1112px grid @1200px+ breakpoint
	$gridColumnWidth1200:     64px !default;
	$gridGutterWidth1200:     40px !default;
	$gridRowWidth1200:        ($gridColumns * $gridColumnWidth1200) + ($gridGutterWidth1200 * ($gridColumns - 1)) !default;

	// 724px grid @768px-979px breakpoints
	$gridColumnWidth768:      45px !default;
	$gridGutterWidth768:      20px !default;
	$gridRowWidth768:         ($gridColumns * $gridColumnWidth768) + ($gridGutterWidth768 * ($gridColumns - 1)) !default;


	// 5 Column Grid
	$grid5ColWidth: 		   204px;
	$grid5GutterWidth: 		   23px;
	$grid5RowWidth: 		   ($grid5Columns * $grid5ColWidth) + ($grid5GutterWidth * ($grid5Columns - 1));





	// Fluid grid
	// -------------------------
	// 

	// Default 980px grid @980px breakpoint
	$fluidContainerMarginWidth:	40px !default;
	$fluidGridColumnWidth:    	percentage($gridColumnWidth/$gridRowWidth) !default;
	$fluidGridGutterWidth:    	percentage($gridGutterWidth/$gridRowWidth) !default;

	// 1200px min
	$fluidContainerMarginWidth1200:	40px !default;
	$fluidGridColumnWidth1200:     	percentage($gridColumnWidth1200/$gridRowWidth1200) !default;
	$fluidGridGutterWidth1200:     	percentage($gridGutterWidth1200/$gridRowWidth1200) !default;

	// 768px-979px
	$fluidContainerMarginWidth768:	40px !default;
	$fluidGridColumnWidth768:      	percentage($gridColumnWidth768/$gridRowWidth768) !default;
	$fluidGridGutterWidth768:      	percentage($gridGutterWidth768/$gridRowWidth768) !default;
	
	// 767px max (mobile)
	$fluidContainerMarginWidth767:	15px !default;

	// 5 Column Grid
	$fluidGrid5ColWidth: percentage($grid5ColWidth / $grid5RowWidth);
	$fluidGrid5GutterWidth: percentage($grid5GutterWidth / $grid5RowWidth);





// Transition Speeds
// --------------------------------------------------
.TRANSITION-SPEEDS{} // this is just here for a SublimeText code-folding collapse point
	$transitionSuperslow:   0.5s;
	$transitionSlower:   	0.4s;
	$transitionSlow:        0.3s;
	$transitionNormal:      0.2s;
	$transitionFast:		0.15s;
	$transitionFaster:      0.10s;
	$transitionSuperfast:   0.05s;


// Transition Timing Functions
// --------------------------------------------------
.TRANSITION-FUNCTIONS{} // this is just here for a SublimeText code-folding collapse point
	$easeOutQuad: 		cubic-bezier(0.250, 0.460, 0.450, 0.940);
	$easeOutCubic: 		cubic-bezier(0.215, 0.610, 0.355, 1.000);
	$easeOutQuart: 		cubic-bezier(0.165, 0.840, 0.440, 1.000);
	$easeOutExpo: 		cubic-bezier(0.190, 1.000, 0.220, 1.000);
	$easeNormal:		ease-out;

