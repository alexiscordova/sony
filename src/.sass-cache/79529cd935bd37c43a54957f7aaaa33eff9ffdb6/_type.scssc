:@template"ò%//
// Typography
// --------------------------------------------------


// Body text
// -------------------------

p {
  margin: 0 0 (0.5em * $baseLineHeight);
  line-height:$baseLineHeight;
  font-size: $font_15_16;
}


// Emphasis & misc
// -------------------------

small {
  font-size: 0.85em; // Since this will be inside of another font-sized element, it just needs to be smaller relative to its parent.
}
strong {} // have to specify this with the specific font you're using - use the font mixins.
em {} // have to specify this with the specific font you're using - use the font mixins.
cite {
  font-style: normal;
}

// Utility classes
.muted {
  color: $grayLight;
}
.text-warning {
  color: $warningText;
}
.text-error {
  color: $errorText;
}
.text-info {
  color: $infoText;
}
.text-success {
  color: $successText;
}


// ----------------------------------
// Headings
// ----------------------------------
h1, h2, h3, h4, h5, h6 {
  margin: (0.25em*$baseLineHeight) 0 (0.25em*$baseLineHeight) 0;
  line-height: $baseLineHeight * 0.7;
  @include fontHeading();
  color: $headingsColor;
  text-rendering: optimizelegibility; // Fix the character spacing for headings
  small {
    font-size:0.85em;
    font-weight: normal;
    line-height: $baseLineHeight * 0.7;
    color: $gray;
  }
  em{
    @include fontHeadingItalic();
  }
}

h1 {
  font-size: $font_54_16;
} 
h2 { 
  font-size: $font_44_16;
}
h3 { 
  font-size:  $font_32_16;
  line-height: $baseLineHeight * 0.75;
} 
h4 { 
  font-size: $font_26_16;
} 
h5 { 
  font-size: $font_18_16;
} 
h6 { 
  font-size: $font_16_16;
}

// ----------------------------------
// Labels
// ----------------------------------
.l1{
  font-size:$font_22_16;
  text-transform:uppercase;
  @include fontBold;
  color: $headingsColor;
  em { @include fontBoldItalic; }
}

.l2, .l3, .l4{
  text-transform:uppercase;
  @include fontMedium;
  em { @include fontMediumItalic; }
}
.l2{
  font-size:$font_15_16;
}
.l3{
  font-size:$font_12_16;
  color: $headingsColor;
}
.l4{
  font-size:$font_11_16;
}


// ----------------------------------
// Paragraphs
// ----------------------------------
.p1{
  font-size:$font_20_16;
  line-height: $baseLineHeight * 0.93;
}
.p2{ /* Standard P tag; P2 class is not necessary, but kept for sake of relative sizing logic */
  // font-size:$font_15_16;
}
.p3{
  font-size:$font_13_16;
}
.p4{
  font-size: $font_12_16;
  @include fontItalic;
}
.p5{
  font-size: $font_11_16;
}



// ----------------------------------
// TextLinks
// ----------------------------------
.tl{
  font-size: $font_13_15;  /* -- Default textlink size is small. */
}
.tl-large{
  font-size: $font_15_15;
}
.tl-arrow-right{
  &:after{
    @include pseudoEl-inline-block();
    @extend .icon-ui-pseudo-el-arrow-right-blue;
    position: relative;
    top: 1px;
    margin-left: 9px;
    margin-top: 5px;
  };
  &.tl-large:after{
    top: 3px;
  };
}

// ---------------------------------
// Global Nav links
// ---------------------------------
.navtray-list-main > li > a.tl {
  @include fontMedium;
  em { @include fontMediumItalic; }
  font-size: $font_13_16; /* -- Default textlink size is small. */
  color:$grayDark;
}



// ----------------------------------
// Blockquotes
// ----------------------------------
blockquote {
  @include fontLightItalic;
  color: $headingsColor;

  p{
    line-height: $baseLineHeight;
  }
  p:first-child{
    text-indent:-14px;
  }
  p:first-child:before{
    content: "\201c";
    line-height:0; /* make sure it doesn't affect the real content. */
  }
  p:last-child:after{
    content: "\201d";
    line-height:0; /* make sure it doesn't affect the real content. */
  }
}
blockquote > p,
blockquote.bq2 > p{
  font-size: $font_28_16;
  line-height: $baseLineHeight * 0.76;    
  letter-spacing:-0.035em;
  &:first-child{
    text-indent:-12px;
  }
}
blockquote.bq1 > p{
  font-size: $font_40_16;
  line-height: $baseLineHeight * 0.76;  
  letter-spacing:-0.025em; 
  &:first-child{
    text-indent:-14px;
  }
}











select,
textarea,
input[type="text"],
input[type="password"],
input[type="datetime"],
input[type="datetime-local"],
input[type="date"],
input[type="month"],
input[type="time"],
input[type="week"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"],
.uneditable-input {
  font-size: $font_13_16; 
  line-height: $baseLineHeight;
}

.text-number-callout-large{
  font-size: $font_60_16;
  font-family: $sansFontFamilyLight;
  @include fontLight;
  em { @include fontLightItalic; }
  color:$headingsColor;
}
.text-promo-title{
  font-size:$font_25_16;
  line-height: $baseLineHeight * 0.8;
  @include fontLight;
  em { @include fontLightItalic; }
  strong{
    @include fontRegular;
    em { @include fontItalic; }
    color:$headingsColor;    
  }
}

// Page header
// -------------------------

.page-header {
  padding-bottom: $baseLineHeight * 0.45;
  margin: $baseLineHeight 0 ($baseLineHeight * 1.5);
  border-bottom: 1px solid $grayLightFieldSmall;
}



// Lists
// --------------------------------------------------

// Unordered and Ordered lists
ul, ol {
  padding: 0;
  margin: 0 0 ($baseLineHeight / 2) * 1em 25px;
}
ul ul,
ul ol,
ol ol,
ol ul {
  margin-bottom: 0;
}
li {
  line-height: $baseLineHeight;
}
ul.unstyled,
ol.unstyled {
  margin-left: 0;
  list-style: none;
}

// Description Lists
dl {
  margin-bottom: $baseLineHeight;
}
dt,
dd {
  line-height: $baseLineHeight;
}
dt {
  font-weight: bold;
}
dd {
  margin-left: $baseLineHeight / 2;
}
// Horizontal layout (like forms)
.dl-horizontal {
  @include clearfix(); // Ensure dl clears floats if empty dd elements present
  dt {
    float: left;
    width: $horizontalComponentOffset - 20;
    clear: left;
    text-align: right;
    @include text-overflow();
  }
  dd {
    margin-left: $horizontalComponentOffset;
  }
}

// MISC
// ----

// Horizontal rules
hr {
  margin: $baseLineHeight 0;
  border: 0;
  border-top: 1px solid $hrBorder;
  border-bottom: 1px solid $white;
}

// Abbreviations and acronyms
abbr[title] {
  cursor: help;
  border-bottom: 1px dotted $grayLight;
}
abbr.initialism {
  font-size: 90%;
  text-transform: uppercase;
}



// Addresses
address {
  display: block;
  margin-bottom: $baseLineHeight;
  font-style: normal;
  line-height: $baseLineHeight;
}






