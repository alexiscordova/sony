// Navs
// --------------------------------------------------
// The original Bootstrap navbar has been renamed simpleNavbar.
// Likewise, the original Bootstrap nav has been renamed simple-nav.

// BASE CLASS
// ----------

.simple-nav {
  margin-left: 0;
  margin-bottom: $baseLineHeight;
  list-style: none;
}

// Make links block level
.simple-nav > li > a {
  display: block;
}
.simple-nav > li > a:hover {
  text-decoration: none;
  background-color: $grayLightFieldSmall;
}

// Redeclare pull classes because of specifity
.simple-nav > .pull-right {
  float: right;
}

// Nav headers (for dropdowns and lists)
.simple-nav-header {
  display: block;
  padding: 3px 15px;
  font-size: 11px;
  font-weight: bold;
  line-height: $baseLineHeight;
  color: $grayLight;
  text-shadow: 0 1px 0 rgba(255,255,255,.5);
  text-transform: uppercase;
}
// Space them out when they follow another list item (link)
.simple-nav li + .simple-nav-header {
  margin-top: 9px;
}



// NAV LIST
// --------

.simple-nav-list {
  padding-left: 15px;
  padding-right: 15px;
  margin-bottom: 0;
}
.simple-nav-list > li > a,
.simple-nav-list .simple-nav-header {
  margin-left:  -15px;
  margin-right: -15px;
  text-shadow: 0 1px 0 rgba(255,255,255,.5);
}
.simple-nav-list > li > a {
  padding: 3px 15px;
}
.simple-nav-list > .active > a,
.simple-nav-list > .active > a:hover {
  color: $white;
  text-shadow: 0 -1px 0 rgba(0,0,0,.2);
  background-color: $linkColor;
}
.simple-nav-list [class^="icon-"] {
  margin-right: 2px;
}
// Dividers (basically an hr) within the dropdown
.simple-nav-list .divider {
  @include nav-divider();
}



// TABS AND PILLS
// -------------

// Common styles
.simple-nav-tabs,
.simple-nav-pills {
  @include clearfix();
}
.simple-nav-tabs > li,
.simple-nav-pills > li {
  float: left;
}
.simple-nav-tabs > li > a,
.simple-nav-pills > li > a {
  padding-right: 12px;
  padding-left: 12px;
  margin-right: 2px;
  line-height: 14px; // keeps the overall height an even number
}

// TABS
// ----

// Give the tabs something to sit on
.simple-nav-tabs {
  border-bottom: 1px solid #ddd;
}
// Make the list-items overlay the bottom border
.simple-nav-tabs > li {
  margin-bottom: -1px;
}
// Actual tabs (as links)
.simple-nav-tabs > li > a {
  padding-top: 8px;
  padding-bottom: 8px;
  line-height: $baseLineHeight;
  border: 1px solid transparent;
  @include border-radius(4px 4px 0 0);
  &:hover {
    border-color: $grayLightFieldSmall $grayLightFieldSmall #ddd;
  }
}
// Active state, and it's :hover to override normal :hover
.simple-nav-tabs > .active > a,
.simple-nav-tabs > .active > a:hover {
  color: $gray;
  background-color: $white;
  border: 1px solid #ddd;
  border-bottom-color: transparent;
  cursor: default;
}


// PILLS
// -----

// Links rendered as pills
.simple-nav-pills > li > a {
  padding-top: 8px;
  padding-bottom: 8px;
  margin-top: 2px;
  margin-bottom: 2px;
  @include border-radius(5px);
}

// Active state
.simple-nav-pills > .active > a,
.simple-nav-pills > .active > a:hover {
  color: $white;
  background-color: $linkColor;
}



// STACKED NAV
// -----------

// Stacked tabs and pills
.simple-nav-stacked > li {
  float: none;
}
.simple-nav-stacked > li > a {
  margin-right: 0; // no need for the gap between nav items
}

// Tabs
.simple-nav-tabs.simple-nav-stacked {
  border-bottom: 0;
}
.simple-nav-tabs.simple-nav-stacked > li > a {
  border: 1px solid #ddd;
  @include border-radius(0);
}
.simple-nav-tabs.simple-nav-stacked > li:first-child > a {
  @include border-top-radius(4px);
}
.simple-nav-tabs.simple-nav-stacked > li:last-child > a {
  @include border-bottom-radius(4px);
}
.simple-nav-tabs.simple-nav-stacked > li > a:hover {
  border-color: #ddd;
  z-index: 2;
}

// Pills
.simple-nav-pills.simple-nav-stacked > li > a {
  margin-bottom: 3px;
}
.simple-nav-pills.simple-nav-stacked > li:last-child > a {
  margin-bottom: 1px; // decrease margin to match sizing of stacked tabs
}



// DROPDOWNS
// ---------

.simple-nav-tabs .dropdown-menu {
  @include border-radius(0 0 6px 6px); // remove the top rounded corners here since there is a hard edge above the menu
}
.simple-nav-pills .dropdown-menu {
  @include border-radius(6px); // make rounded corners match the pills
}

// Default dropdown links
// -------------------------
// Make carets use linkColor to start
.simple-nav .dropdown-toggle .caret {
  border-top-color: $linkColor;
  border-bottom-color: $linkColor;
  margin-top: 6px;
}
.simple-nav .dropdown-toggle:hover .caret {
  border-top-color: $linkColorHover;
  border-bottom-color: $linkColorHover;
}
/* move down carets for tabs */
.simple-nav-tabs .dropdown-toggle .caret {
  margin-top: 8px;
}

// Active dropdown links
// -------------------------
.simple-nav .active .dropdown-toggle .caret {
  border-top-color: $grayDark;
  border-bottom-color: $grayDark;
}
.simple-nav-tabs .active .dropdown-toggle .caret {
  border-top-color: $gray;
  border-bottom-color: $gray;
}

// Active:hover dropdown links
// -------------------------
.simple-nav > .dropdown.active > a:hover {
  cursor: pointer;
}

// Open dropdowns
// -------------------------
.simple-nav-tabs .open .dropdown-toggle,
.simple-nav-pills .open .dropdown-toggle,
.simple-nav > li.dropdown.open.active > a:hover {
  color: $white;
  background-color: $grayLight;
  border-color: $grayLight;
}
.simple-nav li.dropdown.open .caret,
.simple-nav li.dropdown.open.active .caret,
.simple-nav li.dropdown.open a:hover .caret {
  border-top-color: $white;
  border-bottom-color: $white;
  @include opacity(100);
}

// Dropdowns in stacked tabs
.tabs-stacked .open > a:hover {
  border-color: $grayLight;
}



// TABBABLE
// --------


// COMMON STYLES
// -------------

// Clear any floats
.tabbable {
  @include clearfix();
}
.tab-content {
  overflow: auto; // prevent content from running below tabs
}

// Remove border on bottom, left, right
.tabs-below > .simple-nav-tabs,
.tabs-right > .simple-nav-tabs,
.tabs-left > .simple-nav-tabs {
  border-bottom: 0;
}

// Show/hide tabbable areas
// .tab-content > .tab-pane,
.pill-content > .pill-pane {
  // display: none;
  position: absolute;
  left: -9999px;
}
// .tab-content > .active,
.pill-content > .active {
  position: inherit;
  left: auto;
}


// BOTTOM
// ------

.tabs-below > .simple-nav-tabs {
  border-top: 1px solid #ddd;
}
.tabs-below > .simple-nav-tabs > li {
  margin-top: -1px;
  margin-bottom: 0;
}
.tabs-below > .simple-nav-tabs > li > a {
  @include border-radius(0 0 4px 4px);
  &:hover {
    border-bottom-color: transparent;
    border-top-color: #ddd;
  }
}
.tabs-below > .simple-nav-tabs > .active > a,
.tabs-below > .simple-nav-tabs > .active > a:hover {
  border-color: transparent #ddd #ddd #ddd;
}

// LEFT & RIGHT
// ------------

// Common styles
.tabs-left > .simple-nav-tabs > li,
.tabs-right > .simple-nav-tabs > li {
  float: none;
}
.tabs-left > .simple-nav-tabs > li > a,
.tabs-right > .simple-nav-tabs > li > a {
  min-width: 74px;
  margin-right: 0;
  margin-bottom: 3px;
}

// Tabs on the left
.tabs-left > .simple-nav-tabs {
  float: left;
  margin-right: 19px;
  border-right: 1px solid #ddd;
}
.tabs-left > .simple-nav-tabs > li > a {
  margin-right: -1px;
  @include border-radius(4px 0 0 4px);
}
.tabs-left > .simple-nav-tabs > li > a:hover {
  border-color: $grayLightFieldSmall #ddd $grayLightFieldSmall $grayLightFieldSmall;
}
.tabs-left > .simple-nav-tabs .active > a,
.tabs-left > .simple-nav-tabs .active > a:hover {
  border-color: #ddd transparent #ddd #ddd;
  *border-right-color: $white;
}

// Tabs on the right
.tabs-right > .simple-nav-tabs {
  float: right;
  margin-left: 19px;
  border-left: 1px solid #ddd;
}
.tabs-right > .simple-nav-tabs > li > a {
  margin-left: -1px;
  @include border-radius(0 4px 4px 0);
}
.tabs-right > .simple-nav-tabs > li > a:hover {
  border-color: $grayLightFieldSmall $grayLightFieldSmall $grayLightFieldSmall #ddd;
}
.tabs-right > .simple-nav-tabs .active > a,
.tabs-right > .simple-nav-tabs .active > a:hover {
  border-color: #ddd #ddd #ddd transparent;
  *border-left-color: $white;
}



// DISABLED STATES
// ---------------

// Gray out text
.simple-nav > .disabled > a {
  color: $grayLight;
}
// Nuke hover effects
.simple-nav > .disabled > a:hover {
  text-decoration: none;
  background-color: transparent;
  cursor: default;
}
